{% extends '@grr_front/layout_front.html.twig' %}

{% block title %}{{'page.not_found'|trans}}{% endblock %}

{% block body %}
    <div class="card mb-3">
        <div class="card-header">
            <h2>{{'page.not_found'|trans}}</h2>
        </div>
        <div class="card-body">
            {{ status_text }}
            <p>
                <a href="{{ path('grr_homepage') }}">{{'return.homepage'|trans}}</a>
            </p>
            <br />
            <br />
            <img src="{{ asset('bundles/grr/images/totem_grr.png') }}" alt="grr">
        </div>
    </div>
{% endblock %}
